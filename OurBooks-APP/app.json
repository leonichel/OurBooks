{
  "expo": {
    "name": "OurBooks",
    "slug": "OurBooks",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image":"./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E9EEF6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
	  "supportsTablet": true,
	  "infoPlist":{
		  "UIBackgroundModes":["audio"]
	  }
    }
  }
}
